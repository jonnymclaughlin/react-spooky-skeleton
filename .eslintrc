{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "extends": "eslint:recommended",
    "rules": {
        "no-irregular-whitespace ": 0,
        "no-mixed-spaces-and-tabs": 1,
        "react/jsx-no-undef": 1,
        "react/jsx-uses-vars": 1,
        "eqeqeq": 1,
        "no-console": 0
    }
}
